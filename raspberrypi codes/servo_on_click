import RPi.GPIO as g
import time
g.setmode(g.BOARD)
g.steup(18,g.OUT)
pwm = g.PWM(18,50)# use 100 if error
pwm.start(2)
while True:

  
  input("\nPress Enter to continue.") 
  pwm.ChangeDutyCycle(6.5)

  input("\nPress Enter to continue.") 
  pwm.ChangeDutyCycle(10)
  
  
  input("\nPress Enter to continue.") 
  pwm.ChangeDutyCycle(2)
  ans=input("\n continue y/n")
  if ans=='n':
    break
